class Solution:
    def secondLargest(self, arr):
        largest = second = float('-inf')

        for i in range(len(arr)):
            if arr[i] > largest:
                second = largest
                largest = arr[i]
            elif arr[i] > second and arr[i] != largest:
                second = arr[i]

        return second

arr = list(map(int, input().split()))
obj = Solution()
print(obj.secondLargest(arr))
